{
    "version": 2,
    "builds": [
        {
            "src": "api/sign.py",
            "use": "@vercel/python"
        },
        {
            "src": "api/stream.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/sign",
            "dest": "/api/sign.py"
        },
        {
            "src": "/api/stream",
            "dest": "/api/stream.py"
        },
        {
            "src": "/api/health",
            "dest": "/api/sign.py"
        },
        {
            "src": "/",
            "dest": "/api/sign.py"
        }
    ]
}