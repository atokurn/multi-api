{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/unlock",
            "dest": "/api/unlock.js"
        },
        {
            "src": "/api/health",
            "dest": "/api/health.js"
        },
        {
            "src": "/",
            "dest": "/api/health.js"
        }
    ]
}